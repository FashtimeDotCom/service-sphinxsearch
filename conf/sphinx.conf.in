source src_parcel : src_swisssearch
{
    sql_query = \
        SELECT \
        gid as id \
        , search_name as detail \
        , coalesce(gemname,'')||' '||coalesce(name,'') as label \
        , origin as origin \
        , st_box2d(the_geom) as geom_st_box2d \
        , rank as rank \
        , gid as id \
        , SUBSTRING(name FROM '([0-9]+)')::int as num \
        from swiss_search \
        where origin = 'parcel'
}

